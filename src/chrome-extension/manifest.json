{
  "manifest_version": 2,
  "name": "JS Code Browser",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "name": "Browse code for this website"
  },
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "http://*/*",
      "https://*/*",
      "file://*/*"
  ]
}